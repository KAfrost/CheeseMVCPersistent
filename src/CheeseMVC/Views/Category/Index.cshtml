@model List<CheeseMVC.Models.CheeseCategory>


<ul>
    @foreach (var category in Model)
    {
        <li>
            <ul asp-controller="Cheese" asp-action="Category" asp-route-id="@category.ID">@category.Name</ul>
        </li>
    }

</ul>

<p><a asp-controller="Category" asp-action="Add">Add Category</a></p>

